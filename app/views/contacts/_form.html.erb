<%= form_for(@contact) do |f| %>
      First Name : <%= f.text_field :first_name.downcase %><br>
      Last Name : <%= f.text_field :last_name.downcase %><br>
      Email Address : <%= f.text_field :email.downcase %><br>
      Phone Number : <%= f.text_field :phone_number %><br><br>
      
      <%= f.submit "Save New Contact", class: "btn btn-sm btn-primary" %>
    <% end %>